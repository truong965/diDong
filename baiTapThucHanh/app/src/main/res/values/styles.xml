<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomHeader" parent="TextAppearance.Material3.BodyMedium">
        <item name="android:textSize">25sp</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_marginTop">25dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:background">#5B5B5B</item>
    </style>
    <style name="CustomTextEdit" parent="TextAppearance.Material3.BodyMedium">
        <item name="android:textSize">20sp</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_margin">3dp</item>
    </style>
    <style name="CustomButton" parent="TextAppearance.Material3.BodyMedium">
        <item name="android:textSize">20sp</item>
        <item name="android:padding">15dp</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:backgroundTint">#CECECE</item>
        <item name="android:textColor">#000000</item>
        <item name="cornerRadius">0dp</item>
    </style>

</resources>